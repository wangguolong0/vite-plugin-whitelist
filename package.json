{
  "name": "vite-plugin-whitelist",
  "version": "0.1.4",
  "description": "A Vite plugin to restrict incoming requests by IP whitelist (supports env-based config).",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "vite",
    "plugin",
    "whitelist",
    "ip",
    "whitelist"
  ],
  "author": "Wang Guolong",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "tsup": "^6.6.0",
    "@types/node": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wangguolong0/vite-plugin-whitelist.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean",
    "prepare": "npm run build"
  }
}